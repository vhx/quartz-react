{
  "name": "quartz-react",
  "version": "0.0.1",
  "description": "React Components for VHX Quartz",
  "main": "index.js",
  "scripts": {
    "build": "cp demo/public/css/*.css dist/ && rollup -c build/rollup.config.js",
    "dev": "rollup -c build/rollup.demo-config.js --watch",
    "serve": "browser-sync start --server --serveStatic \"demo/public\" --files \"demo/public/**\" --no-open",
    "start": "npm-run-all --parallel dev serve",
    "test": "mocha components/**/*.test.jsx --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastiansandqvist/quartz-react.git"
  },
  "keywords": [
    "vhx",
    "quartz",
    "vimeo",
    "react",
    "components"
  ],
  "author": "VHX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sebastiansandqvist/quartz-react/issues"
  },
  "homepage": "https://github.com/sebastiansandqvist/quartz-react#readme",
  "devDependencies": {
    "@vhx/eslint-config-vhx": "0.0.1",
    "autoprefixer": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "browser-sync": "^2.18.12",
    "chai": "^4.0.2",
    "enzyme": "^2.8.2",
    "jsdom": "7.0.2",
    "mocha": "^3.4.2",
    "mocha-jsdom": "^1.1.0",
    "npm-run-all": "^4.0.2",
    "react-test-renderer": "^15.5.4",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-filesize": "^1.3.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-watch": "^3.2.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
